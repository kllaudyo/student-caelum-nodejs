<html>
    <body>
        <table border='1'>
            <thead>
                <tr>
                    <th>id</th>
                    <th>titulo</th>
                    <th>descrição</th>
                    <th>preco</th>
                </tr>
            </thead>
            <tbody>
            <%for(var i=0,total=lista.length;i<total;i++){%>
                <tr>
                    <td><%=lista[i].id%></td>
                    <td><%=lista[i].titulo%></td>
                    <td><%=lista[i].descricao%></td>
                    <td><%=lista[i].preco%></td>
                </tr>
            <%}%>
            </tbody>
        </table>
    </body>
</html>